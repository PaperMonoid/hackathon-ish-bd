<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns ="urn:nhibernate-mapping-2.2"
                   assembly ="hackathonishbd" namespace = "hackathonishbd.Models">
  <class name ="Usuario" table ='"T_usuarios"'>
    <id name ="IdUsuario">
      <column name ='"ID_usuario"' not-null="true"/>
    </id>
    <property name ="Nombre">
      <column name ='"Nombre"'/>
    </property>
    <property name="Apellido">
      <column name ='"Apellido"'/>
    </property>
    <property name="Clave">
      <column name='"Clave"'/>
    </property>
    <property name="Sexo">
      <column name='"Sexo"'/>
    </property>
    <property name="Rol">
      <column name='"Rol"'/>
    </property>
    <property name ="FechaRegistro">
      <column name ='"Fecha_registro"'/>
    </property>
    <property name="FechaAcceso">
      <column name ='"Fecha_acceso"'/>
    </property>
    <property name="Activo">
      <column name='"B_activo"'/>
    </property>
  </class>
</hibernate-mapping>
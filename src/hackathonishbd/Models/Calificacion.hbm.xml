<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns ="urn:nhibernate-mapping-2.2"
                   assembly ="hackathonishbd" namespace="hackathonishbd.Models">
  <class name ="Calificacion" table ='"T_CALIFICACIONES"'>
    <id name ="IdCalificaciones">
      <column name='"ID_calificaciones"' not-null="true"/>
    </id>
    <property name="IdMaestro">
      <column name='"ID_maestro"'/>
    </property>
    <property name="IdAlumno">
      <column name='"ID_alumno"'/>
    </property>
    <property name="Valor">
      <column name='"Calificacion"'/>
    </property>
    <property name="Final">
      <column name ='"B_Final"'/>
    </property>
    <property name="FechaFinal">
      <column name='"Fecha_Final"'/>
    </property>
  </class>
</hibernate-mapping>